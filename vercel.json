{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "http://44.192.90.179:8080/api/:match*"
    },
    {
      "source": "/ws/:match*",
      "destination": "http://44.192.90.179:8080/ws/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "unsafe-none" }
      ]
    }
  ]
}
